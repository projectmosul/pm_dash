    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1"> 
        <div data-id="visitor_count" data-view="Number" data-title="Visitors This Month"></div> 
    </li>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1"> 
        <div data-id="visitor_count_real_time" data-view="Graph" data-title="Current Visitors"></div> 
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="git_issues_labeled_defects" data-view="Graph" data-title="Issues Count" ></div>
    </li>
  </ul>
</div>